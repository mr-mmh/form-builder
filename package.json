{
    "name": "@mrmmh/form-builder",
    "version": "0.0.2",
    "description": "A reusable form component library using React and TailwindCSS",
    "type": "module",
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./ui": {
            "import": "./dist/ui.mjs",
            "types": "./dist/ui.d.ts"
        }
    },
    "scripts": {
        "dev": "vite --config example/vite.config.ts",
        "build": "tsup",
        "lint": "eslint . --ext .ts,.tsx",
        "release": "changeset",
        "ci:publish": "pnpm build && changeset publish --access public",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "react",
        "form",
        "tailwindcss",
        "component-library",
        "ui",
        "shadcn"
    ],
    "author": "mohammad mahdi heidarnia",
    "license": "ISC",
    "packageManager": "pnpm@10.6.3",
    "engines": {
        "node": ">=18"
    },
    "repository": {
        "url": "https://github.com/mr-mmh/form-builder"
    },
    "peerDependencies": {
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "react": ">=18",
        "react-dom": ">=18",
        "tailwindcss": "^3.4.1",
        "zod": "^3.24.4",
        "react-hook-form": "^7.56.3",
        "tailwind-merge": "^3.2.0",
        "lucide-react": "^0.509.0",
        "@hookform/resolvers": "^5.0.1",
        "@radix-ui/react-label": "^2.1.6",
        "@radix-ui/react-slot": "^1.2.2",
        "@radix-ui/react-radio-group": "^1.3.6",
        "@radix-ui/react-select": "^2.2.4",
        "@radix-ui/react-slider": "^1.3.4",
        "@radix-ui/react-switch": "^1.2.4",
        "@radix-ui/react-tooltip": "^1.2.6",
        "@radix-ui/react-popover": "^1.1.13",
        "fuse.js": "^7.1.0",
        "use-debounce": "^10.0.4",
        "@radix-ui/react-dialog": "^1.1.6",
        "cmdk": "1.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.28.1",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
        "@types/node": "^20.17.24",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@vitejs/plugin-react": "^4.5.0",
        "autoprefixer": "^10.0.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "eslint": "^8.57.1",
        "eslint-config-next": "14.2.21",
        "postcss": "^8.0.0",
        "prettier": "^3.5.3",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "tailwindcss": "^3.4.1",
        "tsup": "^8.4.0",
        "typescript": "^5.8.2",
        "vite": "^5.0.0",
        "zod": "^3.24.4",
        "react-hook-form": "^7.56.3",
        "tailwind-merge": "^3.2.0",
        "lucide-react": "^0.509.0",
        "@hookform/resolvers": "^5.0.1",
        "@radix-ui/react-label": "^2.1.6",
        "@radix-ui/react-slot": "^1.2.2",
        "@radix-ui/react-checkbox": "^1.3.1",
        "@radix-ui/react-radio-group": "^1.3.6",
        "@radix-ui/react-select": "^2.2.4",
        "@radix-ui/react-slider": "^1.3.4",
        "@radix-ui/react-switch": "^1.2.4",
        "@radix-ui/react-tooltip": "^1.2.6",
        "@radix-ui/react-popover": "^1.1.13",
        "fuse.js": "^7.1.0",
        "use-debounce": "^10.0.4",
        "@radix-ui/react-dialog": "^1.1.6",
        "cmdk": "1.0.0"
    }
